{
  "name": "bb-public-library",
  "version": "1.0.0",
  "description": "Library to provide common Blackboard design patterns, utilities, localization/accessibility solutions, and styling to build an embeddable content provider that blends in with the Blackboard Learn UI",
  "main": "index.js",
  "scripts": {
    "lint:fix": "eslint --ext .jsx,.js --fix ./",
    "lint": "eslint --ext .jsx,.js ./",
    "preinstall": "rm -f package-lock.json*",
    "test": "npm run lint:fix && jest --no-cache",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "key-code": "0.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-array-includes": "^2.0.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-each": "^0.3.1",
    "nodemon": "^1.14.11",
    "react-test-renderer": "^16.0.0"
  },
  "jest": {
    "coverageDirectory": "../coverage/",
    "moduleNameMapper": {
      "^.+\\.(css|scss|svg)$": "identity-obj-proxy"
    },
    "setupFiles": [],
    "setupTestFrameworkScriptFile": "./test-setup.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
