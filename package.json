{
  "name": "bb-learn-ultra-embedded-content",
  "version": "1.0.0",
  "description": "Repository for Blackboard partners to build content UI and embed content in Learn Ultra",
  "main": "webpack.parts.js",
  "scripts": {
    "build:all": "sh _bin/apply-to-all.sh 'npm run build'",
    "clean:all": "rm -rf node_modules && sh _bin/apply-to-all.sh 'rm -rf node_modules'",
    "lint:all": "npm install && sh _bin/apply-to-all.sh 'npm run lint'",
    "install:all": "sh _bin/apply-to-all.sh 'npm install'",
    "preinstall": "rm -f package-lock.json*",
    "test:all": "sh _bin/apply-to-all.sh 'npm test'",
    "test:coverage:all": "sh _bin/apply-to-all.sh 'npm run test:coverage'"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^3.1.12",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.3.0",
    "@types/prop-types": "^15.5.4",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "happypack": "^5.0.0",
    "nexus-deployer": "^0.1.8",
    "node-sass": "^4.9.2",
    "path": "^0.12.7",
    "postcss-loader": "^2.1.6",
    "react": "^16.2.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "svg-url-loader": "^2.3.2",
    "tailwindcss": "^0.4.1",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {}
}
